{
    "name": "stock-analysis",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://dirktunak.github.io/react-gh-pages",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --config webpack.dev.js --host localhost --port 8080",
        "build": "webpack --config webpack.prod.js",
        "build:dev": "webpack --config webpack.dev.js",
        "test": "jest",
        "jest": "jest",
        "lint": "eslint 'src/**/*.js'",
        "lint:fix": "eslint 'src/**/*.js' --fix",
        "pre": "npm run lint:fix && npm run jest && npm run build:dev",
        "predeploy": "npm run lint:fix && npm run jest && npm run build",
        "deploy": "gh-pages -d build"
    },
    "dependencies": {
        "history": "^4.10.1",
        "lodash": "^4.17.15",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "styled-components": "^4.4.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.4",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-decorators": "^7.7.4",
        "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
        "@babel/plugin-proposal-function-sent": "^7.7.4",
        "@babel/plugin-proposal-json-strings": "^7.7.4",
        "@babel/plugin-proposal-numeric-separator": "^7.7.4",
        "@babel/plugin-proposal-throw-expressions": "^7.7.4",
        "@babel/plugin-syntax-dynamic-import": "^7.7.4",
        "@babel/plugin-syntax-import-meta": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-styled-components": "^1.10.6",
        "css-loader": "^3.2.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-react": "^7.17.0",
        "gh-pages": "^2.1.1",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "style-loader": "^0.23.1",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0",
        "webpack-merge": "^4.2.2"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "htmlWhitespaceSensitivity": "css",
        "insertPragma": false,
        "jsxBracketSameLine": true,
        "jsxSingleQuote": true,
        "parser": "babel",
        "proseWrap": "preserve",
        "requirePragma": false,
        "semi": false,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "none",
        "useTabs": false,
        "printWidth": 100
    }
}
